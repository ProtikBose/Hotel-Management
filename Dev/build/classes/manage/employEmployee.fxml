<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manage.EmployEmployeeController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@Natural%20tree%20tunnel,%20Portugal.jpg" />
         </image>
      </ImageView>
      <Button layoutX="645.0" layoutY="14.0" mnemonicParsing="false" onAction="#backfunc" text="Back">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <TextField fx:id="roomtext" layoutX="443.0" layoutY="72.0" promptText="Enter Room ID" />
      <Button fx:id="workFunctext" layoutX="492.0" layoutY="109.0" mnemonicParsing="false" onAction="#workFunc" text="Work" />
      <Button layoutX="55.0" layoutY="72.0" mnemonicParsing="false" onAction="#employfunc" prefHeight="124.0" prefWidth="156.0" text="Employ A New One" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <Button layoutX="55.0" layoutY="237.0" mnemonicParsing="false" onAction="#excangeFunc" prefHeight="132.0" prefWidth="170.0" text="Employee An Old One">
         <font>
            <Font size="15.0" />
         </font>
         <effect>
            <InnerShadow />
         </effect>
      </Button>
      <TextField fx:id="prevID" layoutX="422.0" layoutY="175.0" prefHeight="36.0" prefWidth="219.0" promptText="Previous Emplyoee ID" />
      <TextField fx:id="nextID" layoutX="422.0" layoutY="237.0" prefHeight="36.0" prefWidth="219.0" promptText="New Emplyoee ID" />
      <Button fx:id="exbutton" layoutX="506.0" layoutY="303.0" mnemonicParsing="false" onAction="#doEx" text="Exchange" />
      <TextField fx:id="newID" layoutX="422.0" layoutY="175.0" prefHeight="36.0" prefWidth="219.0" promptText="Enter an ID" />
      <Button fx:id="embutton" layoutX="499.0" layoutY="243.0" mnemonicParsing="false" onAction="#doEmp" text="Employ" />
   </children>
</AnchorPane>
