<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manage.RegisterController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@new.jpg" />
         </image>
      </ImageView>
      <Button fx:id="register" layoutX="281.0" layoutY="317.0" mnemonicParsing="false" onAction="#regfunc" prefHeight="37.0" prefWidth="138.0" text="Register">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font></Button>
      <TextField fx:id="namef" layoutX="85.0" layoutY="82.0" prefHeight="25.0" prefWidth="214.0" promptText="Full Name">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="emailf" layoutX="85.0" layoutY="138.0" prefHeight="29.0" prefWidth="312.0" promptText="Email Address">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </TextField>
      <TextField fx:id="phonef" layoutX="425.0" layoutY="82.0" prefHeight="29.0" prefWidth="214.0" promptText="Mobile No">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="addressf" layoutX="85.0" layoutY="239.0" prefHeight="37.0" prefWidth="282.0" promptText="Address">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <PasswordField fx:id="passf" layoutX="85.0" layoutY="186.0" prefHeight="29.0" prefWidth="162.0" promptText="Password">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </PasswordField>
      <TextField fx:id="cityf" layoutX="425.0" layoutY="186.0" promptText="City">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="countryf" layoutX="425.0" layoutY="243.0" promptText="Country">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </TextField>
      <Button layoutX="659.0" layoutY="2.0" mnemonicParsing="false" onAction="#backtologin" text="Back">
         <effect>
            <InnerShadow />
         </effect>
      </Button>
   </children>
</AnchorPane>
